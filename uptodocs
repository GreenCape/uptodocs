#!/usr/bin/env php
<?php

use Verraes\UpToDocs\Main;

$locations = [__DIR__ . '/vendor/autoload.php', __DIR__ . '/../autoload.php', __DIR__ . '/../../vendor/autoload.php'];
foreach ($locations as $location) {
    if (file_exists($location)) {
        require_once $location;
        exit(Main::main());
    }

}
